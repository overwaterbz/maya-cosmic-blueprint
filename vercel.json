{
  "version": 2,
  "builds": [
    {
      "src": "api/minimal_index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/minimal_index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task"
  }
}